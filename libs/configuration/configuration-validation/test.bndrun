-tester: biz.aQute.tester.junit-platform
-runfw: org.apache.felix.framework;
-runee: JavaSE-11
-runtrace: true
-resolve.effective: active

-runsystempackages: \
    javax.xml.stream;version=1.0.0,\
    javax.xml.stream.events;version=1.0.0,\
    javax.xml.stream.util;version=1.0.0,\
    sun.security.x509

-runproperties: \
    org.slf4j.simpleLogger.defaultLogLevel=info,\
    org.slf4j.simpleLogger.showShortLogName=true,\
    org.slf4j.simpleLogger.showThreadName=false,\
    org.slf4j.simpleLogger.showDateTime=true,\
    org.slf4j.simpleLogger.dateTimeFormat='yyyy-MM-dd HH:mm:ss:SSS Z'

-runrequires: \
    bnd.identity;id='net.corda.configuration-validation',\
    bnd.identity;id='net.corda.config-schema',\
    bnd.identity;id='${project.archivesBaseName}-tests',\
    bnd.identity;id='junit-jupiter-engine',\
    bnd.identity;id='slf4j.simple',\
    bnd.identity;id='junit-platform-launcher'
-runstartlevel: \
    order=sortbynameversion,\
    begin=-1
-runbundles: \
	bcprov;version='[1.70.0,1.70.1)',\
	com.fasterxml.jackson.core.jackson-annotations;version='[2.13.1,2.13.2)',\
	com.fasterxml.jackson.core.jackson-core;version='[2.13.1,2.13.2)',\
	com.fasterxml.jackson.core.jackson-databind;version='[2.13.1,2.13.2)',\
	com.networknt.json-schema-validator;version='[1.0.66,1.0.67)',\
	com.typesafe.config;version='[1.4.1,1.4.2)',\
	configuration-validation-tests;version='[5.0.0,5.0.1)',\
	junit-jupiter-api;version='[5.8.2,5.8.3)',\
	junit-jupiter-engine;version='[5.8.2,5.8.3)',\
	junit-platform-commons;version='[1.8.2,1.8.3)',\
	junit-platform-engine;version='[1.8.2,1.8.3)',\
	junit-platform-launcher;version='[1.8.2,1.8.3)',\
	net.corda.base;version='[5.0.0,5.0.1)',\
	net.corda.cipher-suite;version='[5.0.0,5.0.1)',\
	net.corda.config-schema;version='[5.0.0,5.0.1)',\
	net.corda.configuration-core;version='[5.0.0,5.0.1)',\
	net.corda.configuration-validation;version='[5.0.0,5.0.1)',\
	net.corda.crypto;version='[5.0.0,5.0.1)',\
	net.corda.crypto-core;version='[5.0.0,5.0.1)',\
	org.apache.commons.lang3;version='[3.7.0,3.7.1)',\
	org.jetbrains.kotlin.osgi-bundle;version='[1.4.32,1.4.33)',\
	org.opentest4j;version='[1.2.0,1.2.1)',\
	slf4j.api;version='[1.7.32,1.7.33)',\
	slf4j.simple;version='[1.7.32,1.7.33)'