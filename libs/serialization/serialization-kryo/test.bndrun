# Ensure Bnd uses the same Java executable as Gradle.
java: ${project.javaExecutable}

-tester: biz.aQute.tester.junit-platform
-runee: JavaSE-11
-runfw: org.apache.felix.framework
-resolve.effective: active
-runtrace: true

# Enable debugging.
# -runjdb: 5005

-runsystempackages: \
    sun.net.www.protocol.jar,\
    sun.security.x509, \
    javax.xml.stream;version=1.0.0,\
    javax.xml.stream.events;version=1.0.0,\
    javax.xml.stream.util;version=1.0.0
-runrequires: \
    bnd.identity;id='net.corda.serialization-kryo',\
    bnd.identity;id='junit-jupiter-engine',\
    bnd.identity;id='junit-platform-launcher',\
    bnd.identity;id='slf4j.simple',\
    bnd.identity;id='${project.archivesBaseName}-tests'
-runproperties: \
    base.directory=${fileuri;${task.temporaryDir}}
    test.cordapp.version=${project.version}
-runstartlevel: \
    order=sortbynameversion,\
    begin=-1
-runbundles: \
	antlr.osgi;version='[2.7.7,2.7.8)',\
	bcpkix;version='[1.69.0,1.69.1)',\
	bcprov;version='[1.69.0,1.69.1)',\
