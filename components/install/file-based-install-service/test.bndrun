-tester: biz.aQute.tester.junit-platform
-runee: JavaSE-11
-runfw: org.apache.felix.framework
-resolve.effective: active
#-runtrace: true

-runproperties: cpi.path=${task.cpiPath}

-runvm: \
    --add-opens, 'java.base/java.net=ALL-UNNAMED'

# Enable debugging.
# -runjdb: 1044

-runrequires: \
    bnd.identity;id='net.corda.install',\
    bnd.identity;id='net.corda.file-based-install-service',\
    bnd.identity;id='junit-jupiter-engine',\
    bnd.identity;id='junit-platform-launcher',\
    bnd.identity;id='junit-platform-launcher',\
    bnd.identity;id='slf4j.simple',\
    bnd.identity;id='file-based-install-service-tests'

# This will help us keep -runbundles sorted
-runstartlevel: \
    order=sortbynameversion,\
    begin=-1
# The version ranges will change as the versions of
# the artifacts and/or their dependencies change.
-runbundles: \
	bcprov;version='[1.69.0,1.69.1)',\
	com.google.guava;version='[30.1.1,30.1.2)',\
	com.google.guava.failureaccess;version='[1.0.1,1.0.2)',\
	com.typesafe.config;version='[1.4.1,1.4.2)',\
	file-based-install-service-tests;version='[5.0.0,5.0.1)',\
	junit-jupiter-api;version='[5.7.2,5.7.3)',\
	junit-jupiter-engine;version='[5.7.2,5.7.3)',\
	junit-jupiter-params;version='[5.7.2,5.7.3)',\
	junit-platform-commons;version='[1.7.2,1.7.3)',\
	junit-platform-engine;version='[1.7.2,1.7.3)',\
	junit-platform-launcher;version='[1.7.2,1.7.3)',\
	net.corda.base;version='[5.0.0,5.0.1)',\
	net.corda.configuration-core;version='[5.0.0,5.0.1)',\
	net.corda.configuration-read;version='[5.0.0,5.0.1)',\
	net.corda.configuration-read-service;version='[5.0.0,5.0.1)',\
	net.corda.configuration-read-service-impl;version='[5.0.0,5.0.1)',\
	net.corda.crypto;version='[5.0.0,5.0.1)',\
	net.corda.file-based-install-service;version='[5.0.0,5.0.1)',\
	net.corda.file-configuration-read-impl;version='[5.0.0,5.0.1)',\
	net.corda.install;version='[5.0.0,5.0.1)',\
	net.corda.kotlin-stdlib-jdk7.osgi-bundle;version='[1.4.32,1.4.33)',\
	net.corda.kotlin-stdlib-jdk8.osgi-bundle;version='[1.4.32,1.4.33)',\
	net.corda.lifecycle;version='[5.0.0,5.0.1)',\
	net.corda.lifecycle-impl;version='[5.0.0,5.0.1)',\
	net.corda.packaging;version='[5.0.0,5.0.1)',\
	net.corda.registry;version='[5.0.0,5.0.1)',\
	org.apache.felix.scr;version='[2.1.28,2.1.29)',\
	org.jetbrains.kotlin.osgi-bundle;version='[1.4.32,1.4.33)',\
	org.opentest4j;version='[1.2.0,1.2.1)',\
	org.osgi.test.common;version='[1.0.0,1.0.1)',\
	org.osgi.test.junit5;version='[1.0.0,1.0.1)',\
	org.osgi.util.function;version='[1.1.0,1.1.1)',\
	org.osgi.util.promise;version='[1.1.1,1.1.2)',\
	slf4j.api;version='[1.7.32,1.7.33)',\
	slf4j.simple;version='[1.7.32,1.7.33)'

