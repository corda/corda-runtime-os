# Ensure Bnd uses the same Java executable as Gradle.
java: ${project.javaExecutable}

-runee: JavaSE-11
-runfw: org.apache.felix.framework
-resolve.effective: active
-runtrace: true

-runvm: \
    --add-opens, 'java.base/java.net=ALL-UNNAMED'

# Enable debugging.
# -runjdb: 1044

-runsystempackages: \
    javax.xml.stream;version=1.0.0,\
    javax.xml.stream.events;version=1.0.0,\
    javax.xml.stream.util;version=1.0.0,\
    sun.security.x509

-runrequires: \
    bnd.identity;id='net.corda.install',\
    bnd.identity;id='slf4j.simple'

# This will help us keep -runbundles sorted
-runstartlevel: \
    order=sortbynameversion,\
    begin=-1

# The version ranges will change as the versions of
# the artifacts and/or their dependencies change.
-runbundles: \
	antlr.osgi;version='[2.7.7,2.7.8)',\
	bcpkix;version='[1.69.0,1.69.1)',\
	bcprov;version='[1.69.0,1.69.1)',\
	bcutil;version='[1.69.0,1.69.1)',\
	com.fasterxml.classmate;version='[1.5.1,1.5.2)',\
